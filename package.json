{
  "name": "typescript-graphql",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpile-only src/server",
    "start": "node dist/server -r tsconfig-paths/register",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node -r tsconfig-paths/register --transpile-only src/schema",
    "migrate": "prisma migrate dev --preview-feature --name",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^2.16.1",
    "@sendgrid/mail": "^7.4.2",
    "@sentry/node": "^6.1.0",
    "@sentry/tracing": "^6.1.0",
    "apollo-server": "2.20.0",
    "bcrypt": "^5.0.0",
    "graphql": "15.5.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "mustache": "^4.1.0",
    "nexus": "1.0.0",
    "prisma-error-enum": "^0.1.1",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mustache": "^4.1.1",
    "@types/node": "12.19.15",
    "prisma": "^2.16.1",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.1.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
